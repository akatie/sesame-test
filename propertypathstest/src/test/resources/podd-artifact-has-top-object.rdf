<?xml version="1.0"?>
<rdf:RDF xmlns="urn:test:"
     xml:base="urn:test:"
     xmlns:obo="http://purl.obolibrary.org/obo/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">

    <rdf:Description rdf:about="urn:temp:inconsistentArtifact:1">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
		<artifactHasTopObject rdf:resource="urn:temp:object:1960"/>
    </rdf:Description>

    <owl:AnnotationProperty rdf:about="#artifactHasTopObject">
        <rdfs:range rdf:resource="#MyTopObject"/>
        <rdfs:domain rdf:resource="owl:Ontology"/>
    </owl:AnnotationProperty>

    <rdf:Description rdf:about="#MyObject">
	    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    </rdf:Description>
        
    <rdf:Description rdf:about="#MyTopObject">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:subClassOf rdf:resource="#MyObject"/>
    </rdf:Description>
    
    <rdf:Description rdf:about="#NotATopObject">
	    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:subClassOf rdf:resource="#MyObject"/>
    </rdf:Description>
    
    <rdf:Description rdf:about="urn:temp:object:1960">
		<!-- This needs to generate an Error: As NotATopObject is not declared to be a subclass of MyTopObject -->
		<rdf:type rdf:resource="#NotATopObject"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </rdf:Description>

</rdf:RDF>    